        BUF     .addr           ; Memory buffer to copy from or to
        OFFS    .byte           ; Offset into page
        PAGE    .word           ; Starting page to copy from or to
        COUNT   .word           ; Number of bytes to copy
        UNUSED  .byte           ; Make the size 8 bytes
.endstruct

