	SBIT(ACC_4, 0xE0, 4);
	SBIT(ACC_3, 0xE0, 3);
	SBIT(ACC_2, 0xE0, 2);
	SBIT(ACC_1, 0xE0, 1);
	SBIT(ACC_0, 0xE0, 0);
SFR(ADCON0, 0x8E); // A/D control register 0
	#define  ENBI0    0x80
	#define  ENADCI0  0x40
	#define  TMM0     0x20
	#define  EDGE0    0x10
	#define  ADCI0    0x08
	#define  ENADC0   0x04
	#define  ADCS01   0x02
	#define  ADCS00   0x01
SFR(ADCON1, 0x97); // A/D control register 1
